<Window x:Class="LeagueUI.WindowSelecteerSpeler"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LeagueUI"
        mc:Ignorable="d"
        Title="WindowSelecteerSpeler" Height="150" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="auto"></RowDefinition>
        </Grid.RowDefinitions>
        <ListBox Grid.Row="0" Name="SpelersListBox" Margin="5" SelectionChanged="SpelersListBox_SelectionChanged"/>
        <Button Grid.Row="1" Content="Ok" Margin="5" Click="OkButton_Click"/>
    </Grid>
</Window>
